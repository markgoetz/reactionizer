{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "jquery": true,
        "commonjs": true,
		"es6": true
    },
    "extends": ["airbnb"],
    "parserOptions": {
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true,
            "jsx": true
        }
    },
    "plugins": [
        "react"
    ],
	"settings": {
	    "react": {
	      "createClass": "createClass",
	      "pragma": "React",
	      "version": "15.0"
	    }
	}
}
